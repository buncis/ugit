<%= simple_form_for @assignment, url: course_kelas_assignments_path(@course_kelas) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.association :course_kelas %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
